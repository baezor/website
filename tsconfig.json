{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/css/*": ["./src/css/*"],
      "@/layouts/*": ["./src/layouts/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/data/*": ["./src/data/*"],
      "@/components/*": ["./src/components/*"],
      "@/const": ["./src/const.ts"]
    }
  }
}
